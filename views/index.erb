<table class="table table-hover">
  <thead>
    <tr>
      <th>Project's Title</th>
      <th>Assigned Volunteers</th>
    </tr>
  </thead>
  <% @projects_list.each do |project| %>
    <tr>
      <td> <a href="/projects/<%= project.id()%>"><%= project.title() %></a> </td>
      <td> <a href="/projects/<%= project.id()%>"><%=  %></a> </td>
    </tr>
  <% end %>
</table>

<div class="panel" id="add_project">
  <div class="panel-heading">
    <h4>Add a New Project</h4>
  </div>
  <div class="panel-body">
    <form action="/" method="post">
      <div class="form-group">
        <label for="title">Project's Title</label>
        <input id="title" name="title" class="form-control" type="text">
      </div>
      <button type="submit" class="btn-lg">Create Project</button>
    </form>
  </div>
</div>
